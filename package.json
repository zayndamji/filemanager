{
  "name": "filemanager-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "ios-device": "react-native run-ios --device",
    "ios-simulator": "react-native run-ios --simulator",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build-ios": "cd ios && xcodebuild -workspace FileManagerNative.xcworkspace -scheme FileManagerNative -configuration Release -destination generic/platform=iOS archive -archivePath FileManagerNative.xcarchive",
    "web": "expo start --web --clear",
    "web:build": "expo export --platform web",
    "build": "expo export --platform web",
    "serve": "npx serve dist -s -l 7345"
  },
